{
  "name": "clean-architecture-vending-machine",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest --ui",
    "preview": "vite preview",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "test-storybook": "test-storybook --watch",
    "screenshot": "storycap http://localhost:9001 --serverCmd \"cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook --no-open -p 9001\" -e Example/**",
    "static-screenshot": "storycap http://127.0.0.1:9006 --serverCmd \"npx http-server storybook-static --ci -p 9006\" -e Example/**",
    "vrt": "npm run screenshot && dotenv reg-suit run"
  },
  "dependencies": {
    "@storybook/jest": "^0.0.10",
    "@storybook/test-runner": "^0.9.4",
    "dotenv-cli": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/ui": "^0.29.2",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "reg-keygen-git-hash-plugin": "^0.12.1",
    "reg-notify-github-plugin": "^0.12.1",
    "reg-publish-s3-plugin": "^0.12.1",
    "reg-suit": "^0.12.1",
    "storycap": "^4.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.2"
  }
}
